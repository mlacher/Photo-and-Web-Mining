#Create Mask for Divider Frames
Div_Raster<- function (Size, Div_const){
i=0
Div_count<- Size/Div_const
Div_Frame<-c(0)
while (i < Div_const){
i=i+1;
Div_Frame=rbind(Div_Frame,Div_count+Div_Frame[i-1]);
}
return (Div_Frame)
}
